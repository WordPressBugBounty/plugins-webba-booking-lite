{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "date": {
            "type": "string",
            "format": "date"
        },
        "customer": {
            "type": "string",
            "title": "Customer field"
        },
        "status": {
            "enum": ["cancelled", "pending", "paid"]
        },
        "payment": {
            "type": "number"
        },
        "phone": {
            "type": "string"
        },
        "placesBooked": {
            "type": "number",
            "hidden": true
        },
        "createdOn": {
            "type": "string",
            "hidden": true
        },
        "email": {
            "type": "string",
            "hidden": true
        }
    }
}
